<UserControl x:Class="Otor.MsixHero.Ui.Modules.Common.PackageContent.View.Source.StandardSource"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Otor.MsixHero.Ui.Modules.Common.PackageContent.View.Source"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Margin="0 0 0 0">
        <TextBlock Style="{StaticResource DataHeader}" Text="Installation from" />
        <TextBlock Style="{StaticResource DataValue}" Text="Side-loaded package" TextWrapping="Wrap" Margin="0 0 0 10" />
    </StackPanel>
</UserControl>
