<fw:AcrylicWindow x:Class="Otor.MsixHero.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        WindowStyle="None"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        AcrylicWindowStyle="NoIcon"
        ExtendViewIntoTitleBar="True"
        xmlns:regions="http://prismlibrary.com/"
        xmlns:modules="clr-namespace:Otor.MsixHero.App.Modules"
        mc:Ignorable="d"
        TintOpacity="0.2"
        TintColor="#0173C7"
        regions:RegionManager.RegionName="{x:Static modules:RegionNames.Root}"
        Title="MainWindow" Height="800" Width="1024">
</fw:AcrylicWindow>
