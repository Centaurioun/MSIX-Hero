    <Color x:Key="Otor.MsixHero.Colors.Accent.Light">#CDE6F7</Color>
    <SolidColorBrush x:Key="Otor.MsixHero.Brushes.Accent" Color="{StaticResource Otor.MsixHero.Colors.Accent}" />
    <SolidColorBrush x:Key="Otor.MsixHero.Brushes.Accent.Light" Color="{StaticResource Otor.MsixHero.Colors.Accent.Light}" />
</ResourceDictionary>